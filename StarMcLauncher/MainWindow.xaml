<Window x:Class="StarMcLauncher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StarMcLauncher"
        mc:Ignorable="d"
        Title="MainWindow" Height="482" Width="825">
    <Grid>
        <Button Content="Launch!" HorizontalAlignment="Left" Height="35" Margin="503,390,0,0" VerticalAlignment="Top" Width="287" Click="Button_Click"/>
        <ComboBox HorizontalAlignment="Left" Height="24" Margin="503,336,0,0" VerticalAlignment="Top" Width="125" x:Name="VersionsList"/>
        <Frame Margin="-130,10,410,10" x:Name="X"/>
        <TextBox HorizontalAlignment="Left" Margin="503,365,0,0" TextWrapping="Wrap" Text="Player" VerticalAlignment="Top" Width="287" Height="20" x:Name="userName"/>
        <RadioButton Content="OffLine" HorizontalAlignment="Left" Margin="667,322,0,0" VerticalAlignment="Top" Checked="RadioButton_Checked"/>
        <RadioButton Content="OnLine(Microsoft)" HorizontalAlignment="Left" Margin="667,341,0,0" VerticalAlignment="Top" Checked="RadioButton_Checked_1"/>

    </Grid>
</Window>
